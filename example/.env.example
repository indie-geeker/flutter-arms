# Example Environment Configuration Template
# Copy this file to .env, .env.development, .env.staging, or .env.production
# and fill in the actual values for your environment.

# Application Configuration
APP_NAME=FlutterArms Example
APP_CHANNEL=default
APP_ENVIRONMENT=development

# API Configuration
API_BASE_URL=https://api.example.com
API_VERSION=v1
WEB_SOCKET_URL=wss://ws.example.com

# Timeouts (in milliseconds)
CONNECT_TIMEOUT=30000
RECEIVE_TIMEOUT=30000
SEND_TIMEOUT=30000

# Feature Flags
ENABLE_VERBOSE_LOGGING=true
ENABLE_CRASH_REPORTING=false
ENABLE_PERFORMANCE_MONITORING=false
ENABLE_ANALYTICS=false

# Storage Configuration
CACHE_MAX_SIZE_MB=100
ENABLE_ENCRYPTION=false

# Debug Settings (development only)
DEBUG_MODE=true
SHOW_PERFORMANCE_OVERLAY=false

# API Keys (keep these secure!)
# API_KEY=your_api_key_here
# SECRET_KEY=your_secret_key_here
